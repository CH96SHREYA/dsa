 public static void  rotateMatrix(int [][] a,int row,int col){
        int [][] b=new int[row][col];
        int tar_col=col-1;
        for(int i=0;i<row;i++){
            for(int j=0;j<col;j++){
                b[j][tar_col]=a[i][j];
            }
            tar_col--;
        }
        
        for(int i=0;i<row;i++){
            for(int j=0;j<col;j++){
                System.out.print(b[i][j]+" ");
            }
            System.out.println();
        }
    }
    
